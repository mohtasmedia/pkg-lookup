{
  "name": "pkg-lookup",
  "version": "1.0.1",
  "description": "Get statistics on packages",
  "author": "Tom Sturge",
  "keywords": [
    "package",
    "statistics",
    "pkg",
    "lookup",
    "npm",
    "information"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mohtasmedia/pkg-lookup/issues"
  },
  "homepage": "https://github.com/mohtasmedia/pkg-lookup#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohtasmedia/pkg-lookup.git"
  },
  "main": "dist/index.js",
  "scripts": {
    "bump": "standard-version",
    "lint": "eslint . --ext .ts",
    "build": "npm-run-all --silent clean -p compile",
    "clean": "rimraf dist",
    "compile": "ncc build src/cli.ts -o dist -m",
    "release": "npm run build -s && npm run bump && git push --follow-tags origin master && npm publish",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect"
  },
  "bin": {
    "pkg-lookup": "dist/index.js"
  },
  "files": [
    "dist/index.js"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/node": "^14.0.1",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.2.0",
    "jest": "^26.0.1",
    "npm-run-all": "^4.0.0",
    "rimraf": "^3.0.0",
    "snyk": "^1.337.0",
    "standard-version": "^8.0.0",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.2"
  },
  "dependencies": {},
  "synk": true
}
