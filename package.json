{
  "name": "pkg-lookup",
  "version": "0.1.11",
  "description": "Get statistics on packages",
  "author": "Tom Sturge",
  "keywords": [
    "package",
    "statistics",
    "pkg",
    "lookup",
    "npm",
    "information"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mohtasmedia/pkg-lookup/issues"
  },
  "homepage": "https://github.com/mohtasmedia/pkg-lookup#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohtasmedia/pkg-lookup.git"
  },
  "main": "dist/cli.js",
  "scripts": {
    "bump": "standard-version",
    "lint": "npm run lint",
    "build": "npm-run-all --silent clean -p tsc -p minify -p size",
    "minify": "uglifyjs-folder ./dist --config-file '.uglifyconfig.json' -yeo dist -x .js",
    "clean": "rimraf dist",
    "tsc": "tsc",
    "size": "echo \"Gzipped Size: $(cat ./dist/cli.js ./dist/utils.js | gzip-size)\"",
    "release": "npm run build -s && npm run bump && git push --follow-tags origin master && npm publish"
  },
  "bin": {
    "pkg-lookup": "dist/cli.js"
  },
  "files": [
    "dist/cli.js"
  ],
  "devDependencies": {
    "@types/node": "^14.0.1",
    "gzip-size-cli": "^3.0.0",
    "npm-run-all": "^4.0.0",
    "rimraf": "^3.0.0",
    "standard-version": "^8.0.0",
    "typescript": "^3.9.2",
    "uglify-es": "^3.3.9",
    "uglifyjs-folder": "^1.5.4"
  },
  "dependencies": {}
}
