{
  "name": "pkg-lookup",
  "version": "0.1.6",
  "description": "Get statistics on packages",
  "author": "Tom Sturge",
  "license": "MIT",
  "main": "dist/cli.js",
  "module": "dist/cli.es.js",
  "umd:main": "dist/cli.umd.js",
  "scripts": {
    "bump": "standard-version",
    "test": "npm run lint",
    "build": "npm-run-all --silent clean -p rollup -p size",
    "clean": "rimraf dist",
    "rollup": "rollup -c",
    "size": "echo \"Gzipped Size: $(gzip-size $npm_package_main)\"",
    "release": "npm run build -s && npm run bump && git push --follow-tags origin master && npm publish"
  },
  "bin": {
    "pkg-lookup": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohtasmedia/pkg-lookup.git"
  },
  "keywords": [
    "package",
    "statistics",
    "pkg",
    "lookup",
    "npm",
    "information"
  ],
  "bugs": {
    "url": "https://github.com/mohtasmedia/pkg-lookup/issues"
  },
  "homepage": "https://github.com/mohtasmedia/pkg-lookup#readme",
  "devDependencies": {
    "documentation": "^12.0.0",
    "gzip-size-cli": "^3.0.0",
    "npm-run-all": "^4.0.0",
    "rimraf": "^3.0.0",
    "rollup": "^2.0.0",
    "rollup-plugin-preserve-shebangs": "^0.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "standard-version": "^7.0.0"
  }
}
